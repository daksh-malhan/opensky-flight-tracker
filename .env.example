# OpenSky OAuth2 (required)
OPENSKY_CLIENT_ID=replace_me
OPENSKY_CLIENT_SECRET=replace_me

# Postgres (local)
PGHOST=localhost
PGPORT=5432
PGDATABASE=flightlogs
PGUSER=flightview
PGPASSWORD=flightview

# PHX bounding box defaults (edit if you want a different geofence)
GEOFENCE_LAMIN=33.386590
GEOFENCE_LOMIN=-112.036858
GEOFENCE_LAMAX=33.444190
GEOFENCE_LOMAX=-111.893005

# Polling
POLL_SECONDS=30

# Keep the table small (optional)
MAX_ROWS=100
